{
  "name": "express-direct-auth",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "npm run start:static",
    "dev": "npm run dev:static",
    "start:static": "node ./web-server/server.js",
    "dev:static": "nodemon ./web-server/server.js --watch ../../../build/cjs",
    "start:dynamic": "APP_MODE=dynamic node ./web-server/server.js",
    "dev:dynamic": "APP_MODE=dynamic nodemon ./web-server/server.js --watch ../../../build/cjs"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mustache-express": "^1.3.0",
    "cors": "^2.8.5",
    "@okta/jwt-verifier": "^2.1.0",
    "@okta/okta-auth-js": "*"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "concurrently": "^6.0.1"
  }
}
